<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Stacked Bar Chart with Title</title>
    <link rel="stylesheet" href="statisticalGraphs/statisticalGraphs.css" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
  </head>
  <body>
    <div id="products" data-products="<%= JSON.stringify(products) %>"></div>
    <div id="users" data-users="<%= JSON.stringify(users) %>"></div>

    <div id="chartContainer"></div>
    <div class="container">
      <div class="row justify-content-center">
        <div class="col-md-6">
          <div class="card">
            <div class="card-header text-center bg-dark text-light">Users By Country</div>
            <div class="card-body">
              <div id="pieChartContainer"></div>
              <div id="legendContainer"></div>
            </div>
          </div>
        </div>
      </div>
    </div>

    <script src="https://d3js.org/d3.v7.min.js"></script>
    <script src="statisticalGraphs/statisticalGraphs.js"></script>
  </body>
</html>
