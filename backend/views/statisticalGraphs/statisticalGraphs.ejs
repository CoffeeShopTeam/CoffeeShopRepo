<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>Stacked Bar Chart with Title</title>
    <link rel="stylesheet" href="/statisticalGraphs/statisticalGraphs.css" />
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
  </head>
  <body>
    <div id="products" data-products="<%= JSON.stringify(products) %>"></div>
    <div id="users" data-users="<%= JSON.stringify(users) %>"></div>

    <div class="container">
      <div class="row">
        <div class="col-md-6">
          <div class="card mb-4">
            <div class="card-header text-center bg-dark text-light">Users By Country</div>
            <div class="card-body">
              <div id="pieChartContainer"></div>
              <div id="legendContainer"></div>
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="card mb-4">
            <div class="card-header text-center bg-dark text-light">Category Quantity</div>
            <div class="card-body">
              <div id="barChartContainer"></div>
              <div id="legendContainer2"></div>
            </div>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-md-6">
          <div class="card mb-4">
            <div class="card-header text-center bg-dark text-light">Card 3</div>
            <div class="card-body">
              <!-- Content for Card 3 -->
            </div>
          </div>
        </div>
        <div class="col-md-6">
          <div class="card mb-4">
            <div class="card-header text-center bg-dark text-light">Card 4</div>
            <div class="card-body">
              <!-- Content for Card 4 -->
            </div>
          </div>
        </div>
      </div>
    </div>

    <script src="https://d3js.org/d3.v7.min.js"></script>
    <script src="/statisticalGraphs/statisticalGraphs.js"></script>
  </body>
</html>
